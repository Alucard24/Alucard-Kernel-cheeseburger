<HintConfigs>
    <Powerhint>
        <!--video encode 30 fps-->
        <!-- B CPU - Cluster max freq ~1.958 Ghz -->
        <Config
            Id="0x00001203" Enable="true" Target="msm8998"
            Resources="0x40804000, 0x7A6" />

        <!--sustained performance-->
        <!-- B CPU - Cluster min freq uncapped -->
        <!-- L CPU - Cluster min freq uncapped -->
        <!-- B CPU - Cluster max freq ~1.2 GHz -->
        <!-- L CPU - Cluster max freq ~1.2 Ghz -->
        <!-- GPU - min freq 180 Mhz -->
        <!-- GPU - max freq 342 Mhz -->
        <!-- GPUBW freq uncapped -->
        <Config
            Id="0x00001206" Enable="true" Target="msm8998"
            Resources="0x40800000, 0x0, 0x40800100, 0x0, 0x40804000, 0x4E0, 0x40804100, 0x4E0,
            0x4280C000, 0xB4, 0x42810000, 0x156, 0x42814000, 0x0"/>
        <!--vr mode-->
        <!-- B CPU - Cluster min freq ~1.4 Ghz -->
        <!-- L CPU - Cluster min freq ~1.4 Ghz -->
        <!-- B CPU - Cluster max freq ~1.4 Ghz -->
        <!-- L CPU - Cluster max freq ~1.4 Ghz -->
        <!-- GPU - min freq 515 Mhz -->
        <!-- GPU - max freq 515 Mhz -->
        <!-- GPUBW freq 1555 Mhz-->
        <Config
            Id="0x00001207" Enable="true" Target="msm8998"
            Resources="0x40800000, 0x626, 0x40800100, 0x613, 0x40804000, 0x626, 0x40804100, 0x613,
            0x4280C000, 0x203, 0x42810000, 0x203, 0x42814000, 0x2E57"/>

        <!--vr mode sustained performance-->
        <!-- B CPU - Cluster min freq ~1.5 Ghz -->
        <!-- L CPU - Cluster min freq ~1.3 Ghz -->
        <!-- B CPU - Cluster max freq ~1.5 Ghz -->
        <!-- L CPU - Cluster max freq ~1.3 Ghz -->
        <!-- GPU - min freq 414 Mhz -->
        <!-- GPU - max freq 414 Mhz -->
        <!-- GPUBW freq 1017 Mhz -->
        <Config
            Id="0x00001301" Enable="true" Target="msm8998"
            Resources="0x40800000, 0x540, 0x40800100, 0x5c6, 0x40804000, 0x540, 0x40804100, 0x5c6, 0x4280C000, 0x19E,
            0x42810000, 0x19E, 0x42814000, 0x1E4F"/>
    </Powerhint>
</HintConfigs>

